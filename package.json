        "build": "tsc",
        "live": "node build/main.js",
        "prettify": "prettier \"src/**/*.ts\" --write",
        "lint-fix": "eslint \"src/**/*.ts\" --ext .ts --fix",
        "lint": "eslint \"src/**/*.ts\" --ext .ts"
    },
    "keywords": [],
