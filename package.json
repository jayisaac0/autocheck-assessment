        "ajv-keywords": "^5.0.0",
        "dotenv": "^10.0.0",
        "fastify": "^3.9.2",
        "fastify-cors": "^6.0.2",
        "fastify-formbody": "^5.1.0",
        "fastify-helmet": "^5.3.2",
        "fastify-plugin": "^3.0.0",
        "fastify-static": "^4.5.0",
        "fastify-swagger": "^4.12.6",
        "moment": "^2.29.1",
        "moment-timezone": "^0.5.34"
